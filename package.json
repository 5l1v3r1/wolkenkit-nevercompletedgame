{
  "name": "wolkenkit-nevercompletedgame",
  "version": "1.0.0",
  "description": "wolkenkit-nevercompletedgame is a wolkenkit implementation of nevercompletedgame.com.",
  "wolkenkit": {
    "application": "ncg",
    "runtime": {
      "version": "3.0.0"
    },
    "environments": {
      "default": {
        "api": {
          "address": {
            "host": "local.wolkenkit.io",
            "port": 3000
          },
          "allowAccessFrom": "*"
        },
        "fileStorage": {
          "allowAccessFrom": "*"
        },
        "node": {
          "environment": "development"
        }
      }
    }
  },
  "dependencies": {
    "wolkenkit-command-tools": "3.0.0"
  },
  "devDependencies": {
    "roboter": "2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenativeweb/wolkenkit-nevercompletedgame.git"
  },
  "keywords": [
    "wolkenkit"
  ],
  "license": "AGPL-3.0"
}
